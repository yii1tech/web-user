    "keywords": ["yii1", "web", "user", "auth", "event", "model"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yii1tech/web-user/issues",
        "wiki": "https://github.com/yii1tech/web-user/wiki",
        "source": "https://github.com/yii1tech/web-user"
    },
